  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Mon stage en images",
      "request": "launch",
      "program": "lib/main.dart",
      "type": "dart",
      "args": [
        "--dart-define=MSEI_USE_EMULATOR=true"
      ],
    },
    {
      "name": "Mon stage en images (with emulator)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "preLaunchTask": "Open Firebase Emulator Console",
      "args": [
        "--dart-define=MSEI_USE_EMULATOR=true"
      ],
    },
    {
      "name": "Database to excel",
      "request": "launch",
      "type": "debugpy",
      "program": "${workspaceFolder}/resources/admin/database_to_excel.py",
      "cwd": "${workspaceFolder}/resources/admin/",
    },
    {
      "name": "Fix missing users",
      "request": "launch",
      "type": "debugpy",
      "program": "${workspaceFolder}/resources/admin/fix_missing_users.py",
      "cwd": "${workspaceFolder}/resources/admin/",
    }
  ]
}